<div id="order_<%= order.id %>" class="order object">
  <%= order_icon(order) %>
  <h3><%= link_to "Приказ #{order_title(order)}", chair_order_url(order.chair, order) %></h3>
  <div class="status">
    <span class="state_<%= order.state %>"><%= order.state_description %></span>
    <span class="date"><%= time_ago(order, :updated_at) %></span>
  </div>
  <span class="projects-list details"><%= order_projects_list(order) %></span>
</div>
