<% title 'Баллы (аттестационная составляющая) участников', :report %>
<% action_link(link_to_delete(manage_attestation_mark_path(@attestation_mark)), "destroy", "Позволяет удалить баллы участника") if can?(:destroy, @attestation_mark) %>

<div class="attestation_mark_form">
  <%= simple_form_for @attestation_mark, url: manage_attestation_mark_path do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>
  <% end %>
</div>
