<% title 'Достижения участников', :report %>
<% action_link(link_to_delete(manage_student_achievement_path(@student_achievement)), "destroy", "Позволяет удалить достижение участника") if can?(:destroy, @student_achievement) %>

<div class="student_achievement_form">
  <%= simple_form_for @student_achievement, url: manage_student_achievement_path do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>
  <% end %>
</div>
