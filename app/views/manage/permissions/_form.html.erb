<%= form.input :user_id, :collection => User.find(:all).collect { | user | [user.name, user.id] }, :include_blank => true %>
<%= form.input :role, :as => :radio_buttons, :collection => Permission.enum[:role].invert %>
<%= form.input :chair_id, :collection => Chair.find(:all).collect { | chair | [chair.abbr, chair.id] }, :include_blank => true %>
